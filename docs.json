{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#8dacde",
    "light": "#8dacde",
    "dark": "#8dacde"
  },
  "appearance":{
    "default":"dark"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "quickstart",
              {
            "group": "The interface",
            "pages": [
              "interface/search-tab",
              "interface/media-tab",
              "interface/settings-tab"
            ]
          },
              "report"
            ]
          },
          {
            "group": "Core concepts",
            "pages": [
              "core-concepts/processing",
              "core-concepts/visual-search",
              "core-concepts/speech-search",
              "core-concepts/machine-learning"
              
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://docs.getjumper.io",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://discord.getjumper.io/",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://getjumper.io/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/jumper_logo.png",
    "dark": "/logo/jumper_logo.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hello@getjumper.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Jumper website",
      "href": "https://getjumper.io"
    },
    "secondary": {
      "type": "button",
      "label": "Buy a license",
      "href": "https://getjumper.io/#pricing"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "discord": "https://discord.getjumper.io",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}